    "require": {
        "php": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelWhisper\\Whisper\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelWhisper\\Whisper\\Test\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelWhisper\\Whisper\\WhisperServiceProvider"
            ]
        }
    }
